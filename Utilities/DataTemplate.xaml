<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:StockPilot.ViewModel"
                    xmlns:view="clr-namespace:StockPilot.View">

    <DataTemplate DataType="{x:Type vm:StockVM}">
        <view:Stock/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:ProductsVM}">
        <view:Products/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:OrdersVM}">
        <view:Orders/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:FinanceVM}">
        <view:Finance/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:MaterialVM}">
        <view:Material/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:ProductVM}">
        <view:Product/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:OrderVM}">
        <view:Order/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:NewFinnanceVM}">
        <view:NewFinance/>
    </DataTemplate>
</ResourceDictionary>